    "ootb-fields-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/ootbFields/OotbFieldsFieldCustomizer.js",
          "manifest": "./src/extensions/ootbFields/OotbFieldsFieldCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
